{
  "name": "P6_Fisheye",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "webpack",
    "start": "nodemon server.js"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "es6": true
    },
    "parser": "babel-eslint",
    "parserOptions": {
      "ecmaVersion": 2017,
      "sourceType": "module"
    },
    "globals": {
      "require": true
    },
    "rules": {
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "always"
      ],
      "eqeqeq": [
        "error",
        "always",
        {
          "null": "ignore"
        }
      ],
      "array-bracket-spacing": [
        "error",
        "always",
        {
          "singleValue": false,
          "objectsInArrays": false,
          "arraysInArrays": false
        }
      ],
      "object-curly-spacing": [
        "error",
        "always",
        {
          "arraysInObjects": true,
          "objectsInObjects": false
        }
      ],
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "eol-last": [
        "error",
        "always"
      ],
      "lines-between-class-members": ["error", "always", { "exceptAfterSingleLine": true }],
      
      "max-len": [
        "error",
        {
          "code": 70,
          "ignoreStrings": true,
          "ignoreTemplateLiterals": true,
          "ignoreComments": true
        }
      ]
    },
    "extends": "eslint:recommended"
  },
  "eslintIgnore": [
    "webpack.config.js",
    "dist/"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ChristopheRibault/ChristopheRibault_6_03052021.git"
  },
  "keywords": [],
  "author": "Christophe Ribault",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ChristopheRibault/ChristopheRibault_6_03052021/issues"
  },
  "homepage": "https://github.com/ChristopheRibault/ChristopheRibault_6_03052021/",
  "devDependencies": {
    "@babel/core": "^7.14.0",
    "@babel/preset-env": "^7.14.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "css-loader": "^5.2.4",
    "eslint": "^7.25.0",
    "sass": "^1.32.12",
    "sass-loader": "^11.0.1",
    "style-loader": "^2.0.0",
    "webpack": "^5.36.2",
    "webpack-cli": "^4.6.0"
  },
  "dependencies": {
    "axios": "^0.21.1",
    "babel-polyfill": "^6.26.0",
    "express": "^4.17.1",
    "nodemon": "^2.0.7",
    "redux": "^4.1.0"
  }
}
